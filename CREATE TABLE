CREATE TABLE TEAM(
TEAM_ID NUMERIC(25) NOT NULL,
CREATED_AT DATE NOT NULL,
PRIMARY KEY(TEAM_ID)
);

CREATE TABLE OWNER (
TEAM_ID NUMERIC(25) NOT NULL,
OWNER_ID NUMERIC(25) NOT NULL,
ADDRESS VARCHAR(25) NOT NULL,
primary key(OWNER_ID),
foreign key(TEAM_ID) references TEAM_ID 
);

CREATE TABLE PLAYER(
PLAYER_ID NUMERIC(25) NOT NULL,
TEAM_ID VARCHAR(25) NOT NULL,
ROLL NUMERIC(25) NOT NULL,
JERSY_NUMBER NUMERIC(25) NOT NULL,
SCORE_ID NUMERIC(25) NOT NULL,
FOREIGN KEY(TEAM_ID,ROLL,SCORE_ID) REFERENCES TEAM_ID,
FOREIGN KEY(ROLL) REFERENCES ROLL,
FOREIGN KEY(SCORE_ID) REFERENCES SCORE_ID
);


CREATE TABLE SUPPORT_STAFF(
SUPPORT_STAFF_ID NUMERIC(25) NOT NULL,
TEAM_ID NUMERIC(25) NOT NULL,
ROLL_DESC NUMERIC(25) NOT NULL,
PRIMARY KEY(SUPPORT_STAFF_ID)
);




CREATE TABLE SCORE(
SCORE_ID NUMERIC(25) NOT NULL,
MATCH_ID NUMERIC(25) NOT NULL,
RUNS NUMERIC(25) NOT NULL,
WKTS NUMERIC(25) NOT NULL,
CATCHES NUMERIC(25) NOT NULL,
FOREIGN KEY(MATCH_ID) REFERENCES MATCH_ID
);

CREATE TABLE ROLLS(
ROLL_ID NUMERIC(25) NOT NULL,
ROLL_DESC NUMERIC(25) NOT NULL,
primary key(ROLL_ID)
);


CREATE TABLE VENUE (
VENUS_ID NUMERIC(25) NOT NULL,
VENUS_NAME VARCHAR(25) NOT NULL,
CITY VARCHAR(25) NOT NULL,
PRIMARY KEY(VENU_ID)
);
